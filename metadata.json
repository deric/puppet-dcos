{
  "name": "deric-dcos",
  "version": "0.5.1",
  "author": "deric",
  "summary": "Manage DC/OS nodes",
  "license": "Apache-2.0",
  "source": "https://github.com/deric/puppet-dcos.git",
  "project_page": "https://github.com/deric/puppet-dcos",
  "issues_url": "https://github.com/deric/puppet-dcos/issues",
  "dependencies": [
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 1.0.0 < 7.0.0"
    },
    {
      "name": "puppet/archive",
      "version_requirement": ">= 0.4.4 < 3.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "6",
        "7",
        "8",
        "9",
        "10"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "10.04",
        "12.04",
        "14.04",
        "16.04",
        "18.04",
        "20.04"
      ]
    },
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "5",
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    }
  ]
}
